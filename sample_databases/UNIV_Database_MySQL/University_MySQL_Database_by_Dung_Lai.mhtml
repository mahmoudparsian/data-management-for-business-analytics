s { text-decoration: none; background-color: rgba(255, 255, 255, 0.1); bord=
er-color: transparent; }

@media (min-width: 48em) {
  .sidebar-item { padding: 1.5rem; }
  .sidebar-nav-item { padding-left: 1.5rem; padding-right: 1.5rem; }
